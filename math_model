    {
      "cell_type": "code",
      "source": [
        "# Install Dependencies\n",
        "!pip install -q transformers datasets PyPDF2 nltk\n",
        "\n",
        "import os\n",
        "import PyPDF2\n",
        "import glob\n",
        "import re\n",
        "import torch\n",
        "import nltk\n",
        "from datasets import Dataset, DatasetDict\n",
        "from transformers import AutoTokenizer, AutoModelForSeq2SeqLM, Seq2SeqTrainer, Seq2SeqTrainingArguments\n",
        "\n",
        "# Download both 'punkt' and 'punkt_tab' resources\n",
        "nltk.download('punkt')\n",
        "nltk.download('punkt_tab') # Download the punkt_tab resource for sentence tokenization.\n",
        "\n",
        "from nltk.tokenize import sent_tokenize\n",
        "\n",
        "# Paths\n",
        "RESOURCE_DIRS = [\"/content/math_resources\", \"/content/math_textbooks\"]\n",
        "\n",
        "# 1. Extract Text from PDF and TXT files\n",
        "def extract_text_from_file(file_path):\n",
        "    try:\n",
        "        if file_path.endswith(\".pdf\"):\n",
        "            with open(file_path, \"rb\") as f:\n",
        "                reader = PyPDF2.PdfReader(f)\n",
        "                text = \"\\n\".join(page.extract_text() for page in reader.pages if page.extract_text())\n",
        "        elif file_path.endswith(\".txt\"):\n",
        "            with open(file_path, \"r\", encoding=\"utf-8\", errors=\"ignore\") as f:\n",
        "                text = f.read()\n",
        "        else:\n",
        "            return \"\"\n",
        "        return re.sub(r'\\s+', ' ', text).strip()\n",
        "    except Exception as e:\n",
        "        print(f\"❌ Error reading {file_path}: {e}\")\n",
        "        return \"\"\n",
        "\n",
        "# 2. Aggregate and clean all text\n",
        "all_text = []\n",
        "for folder in RESOURCE_DIRS:\n",
        "    files = glob.glob(os.path.join(folder, \"*.pdf\")) + glob.glob(os.path.join(folder, \"*.txt\"))\n",
        "    print(f\"🔍 Found {len(files)} files in {folder}\")\n",
        "    for file in files:\n",
        "        text = extract_text_from_file(file)\n",
        "        if len(text) > 1000:\n",
        "            all_text.append(text)\n",
        "\n",
        "print(f\"✅ Loaded {len(all_text)} raw documents\")\n",
        "\n",
        "# 3. Split into manageable samples (400-800 words per chunk)\n",
        "def chunk_text(text, max_words=800):\n",
        "    sentences = sent_tokenize(text)\n",
        "    chunks, chunk = [], []\n",
        "    word_count = 0\n",
        "    for sentence in sentences:\n",
        "        words = sentence.split()\n",
        "        if word_count + len(words) > max_words:\n",
        "            chunks.append(\" \".join(chunk))\n",
        "            chunk, word_count = [], 0\n",
        "        chunk.extend(words)\n",
        "        word_count += len(words)\n",
        "    if chunk:\n",
        "        chunks.append(\" \".join(chunk))\n",
        "    return chunks\n",
        "\n",
        "samples = []\n",
        "for doc in all_text:\n",
        "    samples.extend(chunk_text(doc))\n",
        "\n",
        "print(f\"📄 Prepared {len(samples)} training chunks\")\n",
        "\n",
        "# 4. Create dataset with dummy tasks (unsupervised, use next-sentence or summary-style proxy task)\n",
        "dataset = Dataset.from_dict({\n",
        "    \"input_text\": [\"summarize: \" + s[:600] for s in samples],\n",
        "    \"target_text\": [s[-300:] for s in samples]\n",
        "})\n",
        "\n",
        "# 5. Tokenize\n",
        "model_name = \"google/mt5-small\"\n",
        "tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "\n",
        "def tokenize_function(batch):\n",
        "    return tokenizer(batch[\"input_text\"], padding=\"max_length\", truncation=True, max_length=512)\n",
        "\n",
        "def tokenize_targets(batch):\n",
        "    labels = tokenizer(batch[\"target_text\"], padding=\"max_length\", truncation=True, max_length=128)\n",
        "    batch[\"labels\"] = labels[\"input_ids\"]\n",
        "    return batch\n",
        "\n",
        "tokenized_dataset = dataset.map(tokenize_function, batched=True)\n",
        "tokenized_dataset = tokenized_dataset.map"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",

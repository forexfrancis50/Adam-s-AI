      "source": [
        "import os\n",
        "import requests\n",
        "from tqdm import tqdm\n",
        "\n",
        "# Create directory for downloads\n",
        "os.makedirs(\"math_resources\", exist_ok=True)\n",
        "\n",
        "def download_file(url, dest_folder=\"math_resources\"):\n",
        "    local_filename = os.path.join(dest_folder, url.split(\"/\")[-1])\n",
        "    try:\n",
        "        with requests.get(url, stream=True) as r:\n",
        "            r.raise_for_status()\n",
        "            with open(local_filename, 'wb') as f:\n",
        "                for chunk in r.iter_content(chunk_size=8192):\n",
        "                    f.write(chunk)\n",
        "        print(f\"✅ Downloaded: {local_filename}\")\n",
        "    except Exception as e:\n",
        "        print(f\"❌ Failed to download {url}: {e}\")\n",
        "\n",
        "# ------------------------\n",
        "# 📘 1. OpenStax Textbooks\n",
        "# ------------------------\n",
        "openstax_pdfs = [\n",
        "    \"https://assets.openstax.org/oscms-prodcms/media/documents/Calculus_Volume_1_-_WEB_68M1Z5W.pdf\",\n",
        "    \"https://assets.openstax.org/oscms-prodcms/media/documents/Calculus_Volume_2_-_WEB.pdf\",\n",
        "    \"https://assets.openstax.org/oscms-prodcms/media/documents/Calculus_Volume_3_-_WEB.pdf\",\n",
        "    \"https://assets.openstax.org/oscms-prodcms/media/documents/Precalculus-OP.pdf\",\n",
        "    \"https://assets.openstax.org/oscms-prodcms/media/documents/Algebra_and_Trigonometry-OP.pdf\",\n",
